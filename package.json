{
  "name": "tiny-httptest",
  "version": "3.0.4",
  "description": "Lightweight HTTP compliant test framework",
  "source": "src/httptest.js",
  "main": "dist/tiny-httptest.cjs",
  "module": "dist/tiny-httptest.esm.js",
  "type": "module",
  "sourceType": "module",
  "types": "types/httptest.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "npm run lint && rm -rf dist/* && npm run rollup",
    "changelog": "auto-changelog -p",
    "lint": "eslint *.js src/*.js test/*.js",
    "mocha": "mocha test/*.js",
    "rollup": "rollup --config",
    "test": "npm run lint && npm run mocha",
    "types": "npx -p typescript tsc src/httptest.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avoidwork/tiny-httptest.git"
  },
  "keywords": [
    "http",
    "https",
    "test",
    "framework",
    "compliant",
    "cors",
    "easy",
    "testing",
    "tests"
  ],
  "author": "Jason Mulligan <jason.mulligan@avoidwork.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/avoidwork/tiny-httptest/issues"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/avoidwork/tiny-httptest",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.24.0",
    "mocha": "^10.0.0",
    "precise": "^2.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "tenso": "^16.2.4",
    "tiny-httptest": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "btoa": "^1.2.1",
    "tiny-coerce": "^2.0.0"
  }
}
